{
  "name": "express-boilerplate",
  "version": "0.1.0",
  "description": "Express Boilerplate",
  "main": "./dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "clean": "rm -rf ./dist",
    "build": "npm run build-ts && npm run copy-static-assets",
    "start:dev": "node -r ts-node/register dist/server.js",
    "test": "mocha -r ts-node/register src/test/**/*.spec.ts --exit --trace-warnings --file src/unhandledRejectionToCatch.ts",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\"",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eldarseytablaev/boilerplate-backend.git"
  },
  "keywords": [
    "boilerplate",
    "backend"
  ],
  "author": "Eldar Sieitablaiev",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/eldarseytablaev/boilerplate-backend/issues"
  },
  "homepage": "https://github.com/eldarseytablaev/boilerplate-backend#readme",
  "dependencies": {
    "@log4js-node/logstashudp": "^1.2.1",
    "axios": "^0.19.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.1",
    "fbgraph": "^1.4.4",
    "http-errors": "^1.7.3",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "log4js": "^6.1.2",
    "moment": "^2.24.0",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.10",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "playfab-sdk": "^2.42.200218",
    "prom-client": "^12.0.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/assert": "^1.4.6",
