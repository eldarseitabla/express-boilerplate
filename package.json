{
  "name": "boilerplate-backend",
  "version": "0.1.0",
  "description": "Boilerplate backend",
  "main": "./dist/server.js",
  "scripts": {
    "start": "NODE_ENV=production npm run serve",
    "start:dev": "NODE_ENV=development npm run build && npm run serve",
    "build": "rm -rf ./dist && npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "test": "NODE_ENV=test mocha test/unit/ --exit --recursive --trace-warnings --file test/unhandledRejectionToCatch.js",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eldarseytablaev/boilerplate-backend.git"
  },
  "keywords": [
    "boilerplate",
    "backend"
  ],
  "author": "Eldar Seytablaev",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/eldarseytablaev/boilerplate-backend/issues"
  },
  "homepage": "https://github.com/eldarseytablaev/boilerplate-backend#readme",
  "dependencies": {
    "@log4js-node/logstashudp": "^1.2.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "log4js": "^6.1.2",
    "playfab-sdk": "^2.42.200218",
    "prom-client": "^12.0.0",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.9",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.7.8",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "chai": "^4.2.0",
    "eslint": "^6.4.0",
    "mocha": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
