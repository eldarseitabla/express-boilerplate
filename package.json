{
  "name": "boilerplate-backend",
  "version": "0.1.0",
  "description": "Boilerplate backend",
  "main": "bin/www",
  "scripts": {
    "start": "NODE_ENV=production LOG4JS_CONFIG=./config/log4js.json node ./src/main.js",
    "test": "NODE_ENV=test mocha test/unit/ --exit --recursive --trace-warnings --file test/unhandledRejectionToCatch.js",
    "lint": "eslint .",
    "start:dev": "NODE_ENV=development LOG4JS_CONFIG=./config/log4js.dev.json node ./src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eldarseytablaev/boilerplate-backend.git"
  },
  "keywords": [
    "boilerplate",
    "backend"
  ],
  "author": "Eldar Seytablaev",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/eldarseytablaev/boilerplate-backend/issues"
  },
  "homepage": "https://github.com/eldarseytablaev/boilerplate-backend#readme",
  "devDependencies": {
    "eslint": "^6.3.0",
    "mocha": "^6.2.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ioredis": "^4.14.1",
    "log4js": "^5.1.0",
    "mongoose": "^5.8.11",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "pg": "^7.12.1",
    "redis-sessions": "^2.1.0",
    "sequelize": "^5.18.4"
  }
}
